' Gambas class file

Static Public FileName As String = "Song/Moonlight Sonata.csv"
Static Public NotesFolder As String = "Piano"
Static Public NewFileName As String = User.Home &/ "Documents/Moonlight Sonata.wav"

Static Public Sub Main()

  Dim WavFile As WavFile = ImportBasicCSV.MergeWavFile_v2(Me.FileName, Me.NotesFolder, 1)

  WavFileWriter.WriteFile(WavFile, NewFileName)

  Sound.Load(NewFileName).Play

End
