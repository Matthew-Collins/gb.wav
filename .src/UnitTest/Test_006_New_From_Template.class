' Gambas class file

Static Public FileName As String = "Piano/3C.wav"
Static Public NewFileName As String = User.Home &/ "Documents/3C (copy).wav"

Static Public Sub Main()

  Dim Model As WavFile = WavFile.GetNewTemplate()

  Model.AppendWavFile(FileName)

  WavFileWriter.WriteFile(Model, NewFileName)

  Assert.Reset
  FileComparer.Compare(NewFileName, FileName)
  Assert.PrintOverall

End
